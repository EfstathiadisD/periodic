{
  "name": "periodic",
  "private": true,
  "packageManager": "pnpm@7.29.0",
  "engines": {
    "node": "19.7.0",
    "npm": "9.5.0"
  },
  "scripts": {
    "publish": "turbo run publish --filter=[origin/main]",
    "build": "turbo run build --filter=[origin/main]",
    "format": "turbo run fmt --filter=[origin/main]",
    "test": "turbo run test --filter=[origin/main]",
    "prepare": "husky install .config/husky",
    "generate": "plop --plopfile ./tools/build/plopfile.mjs"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@shopify/eslint-plugin": "42.0.3",
    "@swc/core": "1.3.38",
    "@swc/jest": "0.2.24",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.35.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "plop": "3.1.2",
    "prettier": "2.8.4",
    "tsup": "6.6.3",
    "turbo": "1.8.3",
    "typescript": "4.9.5"
  }
}
